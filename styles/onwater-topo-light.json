{
  "version": 8,
  "name": "onWater Topo Light",
  "metadata": {
    "description": "onWater topo basemap with contours in feet, public lands, and terrain"
  },
  "sources": {
    "composite": {
      "url": "mapbox://mapbox.mapbox-streets-v8,mapbox.mapbox-terrain-v2",
      "type": "vector"
    },
    "mapbox-dem": {
      "type": "raster-dem",
      "url": "mapbox://mapbox.mapbox-terrain-dem-v1",
      "tileSize": 512,
      "maxzoom": 14
    }
  },
  "terrain": {
    "source": "mapbox-dem",
    "exaggeration": 1.5
  },
  "sprite": "mapbox://sprites/mapbox/outdoors-v12",
  "glyphs": "mapbox://fonts/mapbox/{fontstack}/{range}.pbf",
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": {
        "background-color": "#e8e4dc"
      }
    },
    {
      "id": "hillshade-shadow",
      "type": "fill",
      "source": "composite",
      "source-layer": "hillshade",
      "filter": ["==", ["get", "class"], "shadow"],
      "paint": {
        "fill-color": "#c8c4b8",
        "fill-opacity": [
          "interpolate", ["linear"], ["zoom"],
          6, ["match", ["get", "level"], 94, 0.18, 90, 0.14, 89, 0.11, 78, 0.08, 67, 0.05, 0.03],
          10, ["match", ["get", "level"], 94, 0.15, 90, 0.12, 89, 0.09, 78, 0.06, 67, 0.04, 0.02],
          14, ["match", ["get", "level"], 94, 0.12, 90, 0.1, 89, 0.08, 78, 0.05, 67, 0.03, 0.02]
        ]
      }
    },
    {
      "id": "hillshade-highlight",
      "type": "fill",
      "source": "composite",
      "source-layer": "hillshade",
      "filter": ["==", ["get", "class"], "highlight"],
      "paint": {
        "fill-color": "#ffffff",
        "fill-opacity": [
          "interpolate", ["linear"], ["zoom"],
          6, ["match", ["get", "level"], 94, 0.15, 90, 0.1, 0.05],
          10, ["match", ["get", "level"], 94, 0.12, 90, 0.08, 0.04],
          14, ["match", ["get", "level"], 94, 0.1, 90, 0.06, 0.03]
        ]
      }
    },
    {
      "id": "landcover-grass",
      "type": "fill",
      "source": "composite",
      "source-layer": "landcover",
      "filter": ["==", ["get", "class"], "grass"],
      "paint": {
        "fill-color": "#d8e0d0",
        "fill-opacity": 0.5
      }
    },
    {
      "id": "landcover-wood",
      "type": "fill",
      "source": "composite",
      "source-layer": "landcover",
      "filter": ["==", ["get", "class"], "wood"],
      "paint": {
        "fill-color": "#c8d4bc",
        "fill-opacity": 0.6
      }
    },
    {
      "id": "landcover-scrub",
      "type": "fill",
      "source": "composite",
      "source-layer": "landcover",
      "filter": ["==", ["get", "class"], "scrub"],
      "paint": {
        "fill-color": "#d5ddc8",
        "fill-opacity": 0.5
      }
    },
    {
      "id": "landcover-crop",
      "type": "fill",
      "source": "composite",
      "source-layer": "landcover",
      "filter": ["==", ["get", "class"], "crop"],
      "paint": {
        "fill-color": "#e5e8d8",
        "fill-opacity": 0.4
      }
    },
    {
      "id": "national-park-fill",
      "type": "fill",
      "source": "composite",
      "source-layer": "landuse_overlay",
      "filter": ["==", ["get", "class"], "national_park"],
      "paint": {
        "fill-color": "#c5d4a8",
        "fill-opacity": 0.45
      }
    },
    {
      "id": "national-park-outline",
      "type": "line",
      "source": "composite",
      "source-layer": "landuse_overlay",
      "filter": ["==", ["get", "class"], "national_park"],
      "paint": {
        "line-color": "#8fa068",
        "line-width": ["interpolate", ["linear"], ["zoom"], 8, 0.5, 12, 1.5],
        "line-opacity": 0.7
      }
    },
    {
      "id": "landuse-park-fill",
      "type": "fill",
      "source": "composite",
      "source-layer": "landuse",
      "filter": ["match", ["get", "class"], ["park", "pitch", "grass"], true, false],
      "paint": {
        "fill-color": "#d0dfc0",
        "fill-opacity": 0.5
      }
    },
    {
      "id": "state-park-fill",
      "type": "fill",
      "source": "composite",
      "source-layer": "landuse",
      "filter": ["all", ["==", ["get", "class"], "park"]],
      "paint": {
        "fill-color": "#ccd8a5",
        "fill-opacity": 0.55
      }
    },
    {
      "id": "state-park-outline",
      "type": "line",
      "source": "composite",
      "source-layer": "landuse",
      "filter": ["==", ["get", "class"], "park"],
      "paint": {
        "line-color": "#94a86c",
        "line-width": ["interpolate", ["linear"], ["zoom"], 10, 0.8, 14, 2],
        "line-opacity": 0.6
      }
    },
    {
      "id": "landcover-snow",
      "type": "fill",
      "source": "composite",
      "source-layer": "landcover",
      "filter": ["==", ["get", "class"], "snow"],
      "paint": {
        "fill-color": "#ffffff",
        "fill-opacity": 0.9
      }
    },
    {
      "id": "cemetery",
      "type": "fill",
      "source": "composite",
      "source-layer": "landuse",
      "filter": ["==", ["get", "class"], "cemetery"],
      "paint": {
        "fill-color": "#d8e0cc",
        "fill-opacity": 0.4
      }
    },
    {
      "id": "water",
      "type": "fill",
      "source": "composite",
      "source-layer": "water",
      "paint": {
        "fill-color": "#b8dce8"
      }
    },
    {
      "id": "water-outline",
      "type": "line",
      "source": "composite",
      "source-layer": "water",
      "paint": {
        "line-color": "#8cc4d8",
        "line-width": ["interpolate", ["linear"], ["zoom"], 8, 0.3, 14, 1],
        "line-opacity": 0.6
      }
    },
    {
      "id": "waterway-river",
      "type": "line",
      "source": "composite",
      "source-layer": "waterway",
      "filter": ["==", ["get", "class"], "river"],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "#a0d0e0",
        "line-width": ["interpolate", ["linear"], ["zoom"], 8, 1.5, 14, 4]
      }
    },
    {
      "id": "waterway-stream",
      "type": "line",
      "source": "composite",
      "source-layer": "waterway",
      "filter": ["match", ["get", "class"], ["stream", "canal"], true, false],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "#a8d4e4",
        "line-width": ["interpolate", ["linear"], ["zoom"], 10, 0.5, 14, 2]
      }
    },
    {
      "id": "contour-line",
      "type": "line",
      "source": "composite",
      "source-layer": "contour",
      "filter": ["!=", ["%", ["round", ["*", ["get", "ele"], 3.28084]], 100], 0],
      "paint": {
        "line-color": "#9a8a70",
        "line-width": ["interpolate", ["linear"], ["zoom"], 11, 0.6, 14, 0.8, 16, 1],
        "line-opacity": ["interpolate", ["linear"], ["zoom"], 11, 0, 12, 0.5, 14, 0.6, 16, 0.7]
      }
    },
    {
      "id": "contour-line-index",
      "type": "line",
      "source": "composite",
      "source-layer": "contour",
      "filter": ["==", ["%", ["round", ["*", ["get", "ele"], 3.28084]], 100], 0],
      "paint": {
        "line-color": "#8a7860",
        "line-width": ["interpolate", ["linear"], ["zoom"], 11, 1, 14, 1.3, 16, 1.6],
        "line-opacity": ["interpolate", ["linear"], ["zoom"], 11, 0, 12, 0.6, 14, 0.7, 16, 0.8]
      }
    },
    {
      "id": "contour-label",
      "type": "symbol",
      "source": "composite",
      "source-layer": "contour",
      "filter": ["==", ["%", ["round", ["*", ["get", "ele"], 3.28084]], 100], 0],
      "layout": {
        "text-field": ["concat", ["to-string", ["round", ["*", ["get", "ele"], 3.28084]]], "ft"],
        "text-font": ["DIN Pro Medium", "Arial Unicode MS Regular"],
        "text-size": ["interpolate", ["linear"], ["zoom"], 13, 9, 16, 11],
        "symbol-placement": "line",
        "text-max-angle": 25,
        "text-padding": 5
      },
      "paint": {
        "text-color": "#7a6855",
        "text-halo-color": "rgba(232, 228, 220, 0.8)",
        "text-halo-width": 1.5,
        "text-opacity": ["interpolate", ["linear"], ["zoom"], 13, 0, 14, 0.8]
      }
    },
    {
      "id": "aeroway-polygon",
      "type": "fill",
      "source": "composite",
      "source-layer": "aeroway",
      "filter": ["==", ["geometry-type"], "Polygon"],
      "paint": {
        "fill-color": "#e0dcd4"
      }
    },
    {
      "id": "aeroway-runway",
      "type": "line",
      "source": "composite",
      "source-layer": "aeroway",
      "filter": ["==", ["get", "type"], "runway"],
      "paint": {
        "line-color": "#d8d4cc",
        "line-width": ["interpolate", ["linear"], ["zoom"], 10, 1, 14, 6]
      }
    },
    {
      "id": "building",
      "type": "fill",
      "source": "composite",
      "source-layer": "building",
      "minzoom": 14,
      "paint": {
        "fill-color": "#dcd8d0",
        "fill-opacity": ["interpolate", ["linear"], ["zoom"], 14, 0, 15, 0.6]
      }
    },
    {
      "id": "road-path",
      "type": "line",
      "source": "composite",
      "source-layer": "road",
      "filter": ["match", ["get", "class"], ["path", "track"], true, false],
      "layout": {
        "line-cap": "round"
      },
      "paint": {
        "line-color": "#c4bca8",
        "line-width": ["interpolate", ["linear"], ["zoom"], 12, 0.8, 16, 2],
        "line-dasharray": [2, 1.5],
        "line-opacity": ["interpolate", ["linear"], ["zoom"], 12, 0, 14, 0.7]
      }
    },
    {
      "id": "road-minor-case",
      "type": "line",
      "source": "composite",
      "source-layer": "road",
      "filter": ["match", ["get", "class"], ["street", "street_limited", "service"], true, false],
      "layout": { "line-cap": "round", "line-join": "round" },
      "paint": {
        "line-color": "#d4d0c4",
        "line-width": ["interpolate", ["linear"], ["zoom"], 12, 2, 16, 8],
        "line-opacity": ["interpolate", ["linear"], ["zoom"], 12, 0, 14, 0.6]
      }
    },
    {
      "id": "road-minor",
      "type": "line",
      "source": "composite",
      "source-layer": "road",
      "filter": ["match", ["get", "class"], ["street", "street_limited", "service"], true, false],
      "layout": { "line-cap": "round", "line-join": "round" },
      "paint": {
        "line-color": "#ffffff",
        "line-width": ["interpolate", ["linear"], ["zoom"], 12, 1, 16, 5],
        "line-opacity": ["interpolate", ["linear"], ["zoom"], 12, 0, 14, 1]
      }
    },
    {
      "id": "road-secondary-tertiary-case",
      "type": "line",
      "source": "composite",
      "source-layer": "road",
      "filter": ["match", ["get", "class"], ["secondary", "tertiary"], true, false],
      "layout": { "line-cap": "round", "line-join": "round" },
      "paint": {
        "line-color": "#ccc8bc",
        "line-width": ["interpolate", ["linear"], ["zoom"], 8, 2, 16, 12],
        "line-opacity": 0.7
      }
    },
    {
      "id": "road-secondary-tertiary",
      "type": "line",
      "source": "composite",
      "source-layer": "road",
      "filter": ["match", ["get", "class"], ["secondary", "tertiary"], true, false],
      "layout": { "line-cap": "round", "line-join": "round" },
      "paint": {
        "line-color": "#ffffff",
        "line-width": ["interpolate", ["linear"], ["zoom"], 8, 1, 16, 8]
      }
    },
    {
      "id": "road-primary-case",
      "type": "line",
      "source": "composite",
      "source-layer": "road",
      "filter": ["==", ["get", "class"], "primary"],
      "layout": { "line-cap": "round", "line-join": "round" },
      "paint": {
        "line-color": "#c4c0b4",
        "line-width": ["interpolate", ["linear"], ["zoom"], 6, 2, 16, 14],
        "line-opacity": 0.7
      }
    },
    {
      "id": "road-primary",
      "type": "line",
      "source": "composite",
      "source-layer": "road",
      "filter": ["==", ["get", "class"], "primary"],
      "layout": { "line-cap": "round", "line-join": "round" },
      "paint": {
        "line-color": "#ffffff",
        "line-width": ["interpolate", ["linear"], ["zoom"], 6, 1, 16, 10]
      }
    },
    {
      "id": "road-motorway-trunk-case",
      "type": "line",
      "source": "composite",
      "source-layer": "road",
      "filter": ["match", ["get", "class"], ["motorway", "trunk"], true, false],
      "layout": { "line-cap": "round", "line-join": "round" },
      "paint": {
        "line-color": "#d4c8a8",
        "line-width": ["interpolate", ["linear"], ["zoom"], 5, 2, 16, 16],
        "line-opacity": 0.8
      }
    },
    {
      "id": "road-motorway-trunk",
      "type": "line",
      "source": "composite",
      "source-layer": "road",
      "filter": ["match", ["get", "class"], ["motorway", "trunk"], true, false],
      "layout": { "line-cap": "round", "line-join": "round" },
      "paint": {
        "line-color": "#f8f0dc",
        "line-width": ["interpolate", ["linear"], ["zoom"], 5, 1, 16, 12]
      }
    },
    {
      "id": "admin-state-province",
      "type": "line",
      "source": "composite",
      "source-layer": "admin",
      "filter": ["==", ["get", "admin_level"], 1],
      "paint": {
        "line-color": "#a8a090",
        "line-width": ["interpolate", ["linear"], ["zoom"], 3, 0.5, 10, 1.5],
        "line-dasharray": [4, 2],
        "line-opacity": 0.7
      }
    },
    {
      "id": "admin-country",
      "type": "line",
      "source": "composite",
      "source-layer": "admin",
      "filter": ["==", ["get", "admin_level"], 0],
      "paint": {
        "line-color": "#8a847c",
        "line-width": ["interpolate", ["linear"], ["zoom"], 3, 0.8, 10, 2],
        "line-opacity": 0.8
      }
    },
    {
      "id": "waterway-label",
      "type": "symbol",
      "source": "composite",
      "source-layer": "waterway",
      "layout": {
        "text-field": ["get", "name"],
        "text-font": ["DIN Pro Italic", "Arial Unicode MS Regular"],
        "text-size": ["interpolate", ["linear"], ["zoom"], 10, 10, 16, 14],
        "symbol-placement": "line",
        "text-max-angle": 30
      },
      "paint": {
        "text-color": "#5890a8",
        "text-halo-color": "#d8ecf4",
        "text-halo-width": 1.5
      }
    },
    {
      "id": "water-label",
      "type": "symbol",
      "source": "composite",
      "source-layer": "water",
      "layout": {
        "text-field": ["get", "name"],
        "text-font": ["DIN Pro Italic", "Arial Unicode MS Regular"],
        "text-size": ["interpolate", ["linear"], ["zoom"], 10, 11, 16, 16]
      },
      "paint": {
        "text-color": "#4880a0",
        "text-halo-color": "#c8e4f0",
        "text-halo-width": 1.5
      }
    },
    {
      "id": "poi-park-label",
      "type": "symbol",
      "source": "composite",
      "source-layer": "poi_label",
      "filter": ["match", ["get", "class"], ["park", "nature_reserve"], true, false],
      "layout": {
        "text-field": ["get", "name"],
        "text-font": ["DIN Pro Bold", "Arial Unicode MS Bold"],
        "text-size": ["interpolate", ["linear"], ["zoom"], 10, 10, 14, 14],
        "text-transform": "uppercase",
        "text-letter-spacing": 0.05,
        "text-max-width": 8
      },
      "paint": {
        "text-color": "#6b8c44",
        "text-halo-color": "rgba(240, 245, 235, 0.9)",
        "text-halo-width": 2
      }
    },
    {
      "id": "poi-peak",
      "type": "symbol",
      "source": "composite",
      "source-layer": "poi_label",
      "minzoom": 12,
      "filter": ["all",
        ["==", ["get", "class"], "landmark"],
        ["has", "elevation"]
      ],
      "layout": {
        "icon-image": "mountain",
        "icon-size": ["interpolate", ["linear"], ["zoom"], 12, 0.6, 14, 0.9],
        "icon-allow-overlap": false,
        "text-field": ["concat", ["get", "name"], "\n", ["to-string", ["round", ["*", ["get", "elevation"], 3.28084]]], " ft"],
        "text-font": ["DIN Pro Medium", "Arial Unicode MS Regular"],
        "text-size": ["interpolate", ["linear"], ["zoom"], 12, 9, 14, 11],
        "text-offset": [0, 0.8],
        "text-anchor": "top",
        "text-optional": true,
        "text-padding": 8,
        "symbol-sort-key": ["*", -1, ["coalesce", ["get", "elevation"], 0]],
        "symbol-spacing": 250
      },
      "paint": {
        "text-color": "#5a5248",
        "text-halo-color": "rgba(232, 228, 220, 0.9)",
        "text-halo-width": 1.5,
        "text-opacity": ["interpolate", ["linear"], ["zoom"], 12, 0, 14, 1]
      }
    },
    {
      "id": "road-label",
      "type": "symbol",
      "source": "composite",
      "source-layer": "road",
      "filter": ["match", ["get", "class"], ["motorway", "trunk", "primary", "secondary", "tertiary"], true, false],
      "layout": {
        "text-field": ["get", "name"],
        "text-font": ["DIN Pro Regular", "Arial Unicode MS Regular"],
        "text-size": ["interpolate", ["linear"], ["zoom"], 10, 9, 16, 13],
        "symbol-placement": "line",
        "text-max-angle": 30
      },
      "paint": {
        "text-color": "#6a645c",
        "text-halo-color": "#ffffff",
        "text-halo-width": 2
      }
    },
    {
      "id": "place-village",
      "type": "symbol",
      "source": "composite",
      "source-layer": "place_label",
      "minzoom": 11,
      "filter": ["match", ["get", "type"], ["village", "hamlet", "suburb", "neighbourhood"], true, false],
      "layout": {
        "text-field": ["step", ["zoom"], ["concat", ["get", "name"], "\n○"], 13, ["get", "name"]],
        "text-font": ["DIN Pro Regular", "Arial Unicode MS Regular"],
        "text-size": ["interpolate", ["linear"], ["zoom"], 10, 11, 16, 14],
        "text-max-width": 8,
        "text-anchor": "bottom",
        "text-justify": "center"
      },
      "paint": {
        "text-color": "#5a5450",
        "text-halo-color": "#e8e4dc",
        "text-halo-width": 1.5
      }
    },
    {
      "id": "place-town",
      "type": "symbol",
      "source": "composite",
      "source-layer": "place_label",
      "minzoom": 10,
      "filter": ["==", ["get", "type"], "town"],
      "layout": {
        "text-field": ["step", ["zoom"], ["concat", ["get", "name"], "\n○"], 11, ["get", "name"]],
        "text-font": ["DIN Pro Medium", "Arial Unicode MS Regular"],
        "text-size": ["interpolate", ["linear"], ["zoom"], 8, 11, 14, 15],
        "text-max-width": 8,
        "text-anchor": "bottom",
        "text-justify": "center"
      },
      "paint": {
        "text-color": "#4a4640",
        "text-halo-color": "#e8e4dc",
        "text-halo-width": 2
      }
    },
    {
      "id": "place-capital",
      "type": "symbol",
      "source": "composite",
      "source-layer": "place_label",
      "minzoom": 4,
      "filter": ["all", ["==", ["get", "type"], "city"], ["==", ["get", "capital"], 2]],
      "layout": {
        "text-field": ["concat", ["get", "name"], "\n⊙"],
        "text-font": ["DIN Pro Medium", "Arial Unicode MS Regular"],
        "text-size": ["interpolate", ["linear"], ["zoom"], 4, 11, 10, 15],
        "text-max-width": 8,
        "text-anchor": "bottom",
        "text-justify": "center"
      },
      "paint": {
        "text-color": "#3a3632",
        "text-halo-color": "#e8e4dc",
        "text-halo-width": 2
      }
    },
    {
      "id": "place-city-small",
      "type": "symbol",
      "source": "composite",
      "source-layer": "place_label",
      "minzoom": 7,
      "filter": ["all", ["==", ["get", "type"], "city"], ["!=", ["get", "capital"], 2], ["<", ["get", "symbolrank"], 13]],
      "layout": {
        "text-field": ["step", ["zoom"], ["concat", ["get", "name"], "\n○"], 10, ["get", "name"]],
        "text-font": ["DIN Pro Medium", "Arial Unicode MS Regular"],
        "text-size": ["interpolate", ["linear"], ["zoom"], 5, 11, 14, 18],
        "text-max-width": 8,
        "text-anchor": "bottom",
        "text-justify": "center"
      },
      "paint": {
        "text-color": "#3a3632",
        "text-halo-color": "#e8e4dc",
        "text-halo-width": 2
      }
    },
    {
      "id": "place-city-large",
      "type": "symbol",
      "source": "composite",
      "source-layer": "place_label",
      "minzoom": 3,
      "filter": ["all", ["==", ["get", "type"], "city"], ["!=", ["get", "capital"], 2], [">=", ["get", "symbolrank"], 13]],
      "layout": {
        "text-field": ["step", ["zoom"], ["concat", ["get", "name"], "\n○"], 8, ["get", "name"]],
        "text-font": ["DIN Pro Bold", "Arial Unicode MS Bold"],
        "text-size": ["interpolate", ["linear"], ["zoom"], 3, 12, 12, 22],
        "text-max-width": 8,
        "text-anchor": "bottom",
        "text-justify": "center"
      },
      "paint": {
        "text-color": "#2a2622",
        "text-halo-color": "#e8e4dc",
        "text-halo-width": 2
      }
    },
    {
      "id": "natural-label",
      "type": "symbol",
